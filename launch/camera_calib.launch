<launch>
<node name="camera" pkg="usb_cam" type="usb_cam_node" output="screen">
    <param name="video_device" value="/dev/video1" />
    <param name="image_width" value="1280" />
    <param name="image_height" value="720" />
    <param name="pixel_format" value="yuyv" />
    <param name="framerate" value="30" />
    <param name="camera_frame_id" value="usb_cam" />
    <param name="io_method" value="mmap" />
    <param name="camera_info_url" type="string" value="file://$(find servofish_ros)/launch/camera.yaml" />
</node>

</node name="camera_calibration" pkg="camera_calibration" type="cameracalibrator.py" args="--size 9x7 -
-square 0.022 image:=/camera/image_raw camera:=/camera --no-service-check"/>

</launch>
rosrun camera_calibration cameracalibrator.py --size 9x7 -
-square 0.022 image:=/camera/image_raw camera:=/camera --no-service-check
